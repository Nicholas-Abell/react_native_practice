{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
    }
  },
  "include": [
    "constants/*",
    "assets/*",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts", 
    "app/index.tsx", "app/_layout.tsx", "app/user/index.jsx"]
}
